{
    "extends": [

        "eslint:recommended",
        "plugin:import/errors",
        "plugin:jsx-a11y/recommended",
        "prettier"
    ],

    "rules": {
        "react/propt-type": 0,
        "no-console": 1
    },
    
    "plugins": [ "import", "jsx-a11y" ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    
    "env": {
        "es6": true,
        "browser": true,
        "node": true 
    }
}